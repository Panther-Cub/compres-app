{
  "name": "compress",
  "version": "0.2.2",
  "description": "A minimalistic video compression app for web optimization",
  "main": "build/main.js",
  "homepage": "./",
  "private": true,
  "author": "J-smizzy",
  "dependencies": {
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.7",
    "@radix-ui/react-switch": "^1.2.6",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "electron-updater": "^6.6.2",
    "ffmpeg-static": "^5.1.0",
    "ffprobe-static": "^3.1.0",
    "fluent-ffmpeg": "^2.1.2",
    "framer-motion": "^12.23.12",
    "lucide-react": "^0.525.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@types/fluent-ffmpeg": "^2.1.27",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.0",
    "electron": "^25.3.0",
    "electron-builder": "^24.6.3",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.0.0",
    "wait-on": "^7.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && tsc -p tsconfig.electron.json && cp -r src/electron build/",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron": "NODE_ENV=development electron .",
    "electron-dev": "concurrently \"npm start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron-build": "npm run build && electron-builder",
    "dist": "npm run build && electron-builder --publish=never",
    "dist:unsigned": "ALLOW_UNSIGNED_UPDATES=true npm run build && electron-builder --publish=never",
    "dist:dev": "ALLOW_UNSIGNED_UPDATES=true npm run build && electron-builder --config electron-builder-dev.yml --publish=never",
    "publish": "npm run build && electron-builder --publish=always",
    "publish:unsigned": "ALLOW_UNSIGNED_UPDATES=true npm run build && electron-builder --publish=always",
    "publish:dev": "ALLOW_UNSIGNED_UPDATES=true npm run build && electron-builder --config electron-builder-dev.yml --publish=always",
    "bump": "node scripts/version-manager.js bump patch",
    "bump:minor": "node scripts/version-manager.js bump minor",
    "bump:major": "node scripts/version-manager.js bump major",
    "bump:beta": "node scripts/version-manager.js bump patch beta",
    "bump:alpha": "node scripts/version-manager.js bump patch alpha",
    "bump:prerelease": "node scripts/version-manager.js bump prerelease",
    "release": "node scripts/version-manager.js release",
    "version:current": "node scripts/version-manager.js current",
    "version:latest": "node scripts/version-manager.js latest",
    "version:help": "node scripts/version-manager.js help"
  },
  "build": {
    "appId": "com.pantherandcub.compress",
    "productName": "Compress",
    "directories": {
      "output": "dist"
    },
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "assets/**/*"
    ],
    "extraResources": [
      {
        "from": "assets",
        "to": "assets"
      }
    ],
    "asarUnpack": [
      "node_modules/ffmpeg-static/**/*",
      "node_modules/ffprobe-static/**/*"
    ],
    "extends": null,
    "publish": [
      {
        "provider": "github",
        "owner": "Panther-Cub",
        "repo": "compress-app",
        "private": false
      }
    ],
    "electronUpdaterCompatibility": ">=5.0.0",
    "mac": {
      "category": "public.app-category.productivity",
      "icon": "assets/Vanilla.icns",
      "target": "zip",
      "artifactName": "${productName}-${version}-mac.${ext}",
      "identity": null,
      "hardenedRuntime": false,
      "gatekeeperAssess": false,
      "notarize": false,
      "type": "distribution",
      "electronLanguages": [
        "en"
      ],
      "entitlements": null,
      "entitlementsInherit": null
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}